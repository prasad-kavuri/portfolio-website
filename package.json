{
    "name": "rag-pipeline-portfolio",
    "version": "1.0.0",
    "description": "RAG Pipeline Demo with Testing Suite",
    "main": "index.html",
    "scripts": {
        "test": "jest",
        "test:unit": "jest",
        "test:e2e": "playwright test",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:ui": "open test-suite.html",
        "lint": "eslint *.js",
        "lint:fix": "eslint *.js --fix",
        "serve": "http-server -p ${PORT:-8081} -c-1 .",
        "validate": "html-validate *.html",
        "test:all": "npm run test:unit && npm run test:e2e"
    },
    "keywords": [
        "rag",
        "pipeline",
        "ai",
        "portfolio"
    ],
    "author": "Prasad Kavuri",
    "license": "MIT",
    "devDependencies": {
        "@playwright/test": "^1.40.0",
        "eslint": "^8.56.0",
        "html-validate": "^8.7.0",
        "http-server": "^14.1.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/prasad-kavuri/portfolio-website.git"
    },
    "bugs": {
        "url": "https://github.com/prasad-kavuri/portfolio-website/issues"
    },
